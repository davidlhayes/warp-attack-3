<!DOCTYPE html>
<html ng-app="warp">
  <head>
    <meta charset="utf-8">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" integrity="sha256-MfvZlkHCEqatNoGiOXveE8FIwMzZg4W85qfrfIFBfYc= sha512-dTfge/zgoMYpP7QbHy4gWMEGsbsdZeCXz7irItjcC3sPUFtf0kuFbDz/ixG7ArTxmDjLXDmezHubeNikyKGVyQ==" crossorigin="anonymous">
    <link rel="stylesheet" href="public/stylesheets/main.css" media="screen" title="no title" charset="utf-8">
    <script src="https://code.jquery.com/jquery-2.1.4.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js" integrity="sha256-Sk3nkD6mLTMOF0EOpNtsIry+s1CsaqQC1rVLTAy+0yc= sha512-K1qjQ+NcF2TYO/eI3M6v8EiNYZfA95pQumfvcVrTHtwQVDG+aHRqLi/ETn2uB+1JqwYqVG3LIvdm9lj6imS/pQ==" crossorigin="anonymous"></script>
    <script type="text/javascript" src="public/scripts/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.6/angular-route.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.6/angular-animate.min.js"></script>
    <script src="public/scripts/angular-app.js"></script>
    <title>Warp Attack</title>
  </head>
  <body ng-controller="BoardCtrl">
    <div class="wrapper">

    <!-- title -->
    <div class="row">
      <div class="col-lg-12">
        <h1>Warp Attack</h1>
      </div>
    </div>
    <!-- board row -->
    <div class="row">
      <div class="col-lg-2" id="lft"></div>

      <div class="col-lg-8" id="rgt">
        <div id="main" class="table">
          <div class="cell" id="r{{space.row}}c{{space.col}}"  ng-repeat="space in spaces | orderBy: sortCol | orderBy: sortRow">
                <div class="token" id="{{space.id}}"></div>
          </div>
      </div>
      </div>

      <div class"col-lg-2" id="lft"></div>
    </div>
    <button class="btn btn-default" ng-click="switchIt(true)">SWITCH</button>

  </div>

  </body>
</html>
